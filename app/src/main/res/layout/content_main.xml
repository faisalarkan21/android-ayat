<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgutama"
    tools:context="com.example.faisal.ayat_syafiq.MenuSubBab3">

    <Button
        android:id="@+id/btnAsalUsulManusia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Asal-Usul Manusia dan Sifat-Sifatnya"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.149" />

    <Button
        android:id="@+id/btnDinamikaKehidupan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dinamika Kehidupan Manusia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.298" />

    <Button
        android:id="@+id/btnJiwaAkalNafsu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jiwa, Akal, dan Nafsu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.447" />

</android.support.constraint.ConstraintLayout>
